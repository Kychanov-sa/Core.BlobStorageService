{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "StorageService": {
    "StorageId": "Storage.0",
    "StorageMode": "Persistent",
    "EnableEmptyDirectoriesTruncation": false,
    "EnableDeleteToRecycleBin": false,
    "EnableDeletionOfExpiredBlobs": false,
    "BlobRetention": "3:00:00:00"
  },

  "FileStorage": {
    "StorageDirectory": "",
    "PartitionsCount": 1024,
    "BlobMaxSizeBytes": 4194304
  }
}
